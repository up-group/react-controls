{
  "name": "@up-group/react-controls",
  "version": "0.1.62",
  "description": "",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Up-Group/react-controls.git"
  },
  "keywords": [
    "UI",
    "React",
    "Health",
    "UpGroup"
  ],
  "author": "@UpGroup",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Up-Group/react-controls/issues"
  },
  "engines": {
    "node": "*",
    "npm": "*"
  },
  "homepage": "https://github.com/Up-Group/react-controls#readme",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/testing/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/config/testing/__mocks__/styleMock.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/",
      "/__mocks__/"
    ],
    "transform": {
      ".*": "<rootDir>/config/testing/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  },
  "scripts": {
    "tsc": "./node_modules/typescript/bin/tsc",
    "version": "tsc -v",
    "clean": "rimraf dist && rimraf test/dist",
    "build": "tsc && copyfiles -u 1 src/Common/theming/icons/*.* dist/src && npm run copyCSS && npm run copyFont && npm run copyImg && npm run copySCSS",
    "copyCSS": "copyfiles -u 1 src/**/*.css dist/src/",
    "copySCSS": "copyfiles -u 1 src/**/*.scss dist/src/",
    "copyFont": "copyfiles -u 1 src/**/*.ttf dist/src/ && copyfiles -u 1 src/**/*.woff dist/src/ && copyfiles -u 1 src/**/*.eot dist/src/",
    "copyImg": " copyfiles -u 1 src/**/*.png dist/src/",
    "build-watch": "tsc -w",
    "prepublish": "npm run clean && npm run build ",
    "publishProd": "npm version patch && npm publish",
    "publishBeta": "npm version patch && npm publish --tag beta",
    "build-test": "npm run clean && webpack",
    "build-test-watch": "npm run clean && webpack --watch",
    "publishLocalToSchemaForm": "copyfiles dist/*.* dist/**/* package.json src/*.* src/**/* src/theming/icons/* ../react-schemaform/node_modules/@up-group/react-controls/",
    "publishLocalToApp": "copyfiles dist/src/*.* dist/src/**/* src/theming/icons/* ../../loopback/revolution-digitale/client/node_modules/@up-group/react-controls/dist",
    "test": "jest --notify --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./docs"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@storybook/addon-actions": "^4.0.0",
    "@storybook/addon-info": "^4.0.0",
    "@storybook/addon-knobs": "^4.0.0",
    "@storybook/addon-links": "^4.0.0",
    "@storybook/react": "^4.0.0",
    "@types/chai": "^3.5.2",
    "@types/chai-enzyme": "^0.6.1",
    "@types/enzyme": "*",
    "@types/es6-promise": "0.0.33",
    "@types/jest": "^21.1.5",
    "@types/jquery": "^2.0.41",
    "@types/node": "^7.0.12",
    "@types/pure-render-decorator": "^0.2.27",
    "@types/react": "^16.0.0",
    "@types/react-dates": "^17.1.2",
    "@types/react-dom": "^16.0.0",
    "@types/react-select": "^2.0.0",
    "@types/webpack-env": "^1.13.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "copyfiles": "^1.2.0",
    "css-loader": "^1.0.1",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.6.0",
    "file-loader": "^2.0.0",
    "jest": "^21.1.5",
    "json-loader": "^0.5.7",
    "markdown-loader": "^4.0.0",
    "node-sass": "^4.10.0",
    "npm": "^6.4.1",
    "react-addons-test-utils": "^15.5.1",
    "react-storybook-addon-info": "^3.5.0",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
