    "react-markdown": "^2.4.5",
    "react-maskedinput": "^3.3.4",
    "react-pure-render": "^1.0.2",
    "react-select": "^2.0.0",
    "react-sortable-hoc": "^0.6.1",
    "react-textarea-autosize": "^7.1.0-1",
    "react-tooltip": "^3.3.0",
    "typestyle": "^1.3.7",
    "validator": "^7.0.0",
    "yup": "^0.26.6"
  }
}
