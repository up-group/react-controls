{
  "name": "@up-group-ui/react-controls",
  "version": "0.2.237",
  "description": "Up shared react controls",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "yarn@3.1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Up-Group/react-controls.git"
  },
  "keywords": [
    "UI",
    "React",
    "Health",
    "UpGroup",
    "TypeScript",
    "TypeStyle"
  ],
  "author": "@UpGroup",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Up-Group/react-controls/issues"
  },
  "engines": {
    "node": "*",
    "npm": "*"
  },
  "homepage": "https://github.com/Up-Group/react-controls#readme",
  "jest": {
    "setupFiles": [
      "<rootDir>/config/testing/shim.js",
      "<rootDir>/config/testing/setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/testing/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/config/testing/__mocks__/styleMock.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules|dist|generators)/",
      "/__mocks__/"
    ],
    "transform": {
      ".*": "<rootDir>/config/testing/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "verbose": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "scripts": {
    "tsc": "./node_modules/typescript/bin/tsc",
    "version": "tsc -v",
    "clean": "rimraf dist && rimraf test/dist",
    "build": "tsc -v && tsc && copyfiles -u 1 src/Common/theming/icons/*.* dist/ && npm run copyCSS && npm run copyFont && npm run copyImg && npm run copySvg",
    "copyCSS": "copyfiles -u 1 src/**/*.css dist/",
    "copyFont": "copyfiles -u 1 src/**/*.ttf dist/ && copyfiles -u 1 src/**/*.woff dist/ && copyfiles -u 1 src/**/*.eot dist/",
    "copyImg": " copyfiles -u 1 src/**/*.png dist/",
    "copySvg": " copyfiles -u 1 src/**/*.svg dist/",
    "build-watch": "tsc -w",
    "prepublishOnly": "npm run clean && npm run build",
    "publishProd": "npm version patch && npm publish --access public",
    "publishBeta": "npm version patch && npm publish --access public --tag beta && git push",
    "build-test": "npm run clean && webpack",
    "build-test-watch": "npm run clean && webpack --watch",
    "publishLocalToSchemaForm": "copyfiles dist/*.* dist/**/* package.json src/*.* src/**/* src/theming/icons/* ../react-schemaform/node_modules/@up-group/react-controls/",
    "publishLocalToApp": "copyfiles dist/src/*.* dist/src/**/* src/theming/icons/* ../../loopback/revolution-digitale/client/node_modules/@up-group/react-controls/dist",
    "test": "jest --notify --detectOpenHandles",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "start": "start-storybook -p 6006",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./public",
    "build-storybook-docs": "start-storybook --docs --no-manager-cache",
    "upgrade": "npx npm-check-updates '/storybook/' -u && npm install",
    "plop": "plop --plopfile ./generators/plopfile.js",
    "lint": "eslint ./src",
    "lint:fix": "cross-env-shell TIMING=1 npm run lint -- --fix",
    "format": "prettier --write './src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
